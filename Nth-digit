
class Solution {
    public int findNthDigit(int n) {
         String str="";
        for(int i=1;i<=n;i++){
            str+=i;
        }
        String val="";
        for(int i=1;i<=str.length();i++){
          if(i==n){
            val+=str.charAt(n-1);
            break;
        }
    }
    int val1=Integer.parseInt(val);
    return val1;
    }
}
